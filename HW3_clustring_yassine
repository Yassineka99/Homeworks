!pip install nltk
from nltk.tokenize import word_tokenize
import nltk
from nltk.corpus import stopwords
from nltk.stem import SnowballStemmer
from nltk.tokenize import word_tokenize
from sklearn.feature_extraction.text import TfidfVectorizer
from sklearn.cluster import KMeans
import pandas as pd

# Download French stopwords
nltk.download('stopwords')
french_stopwords = set(stopwords.words('french'))

# Download French snowball stemmer
nltk.download('punkt')
nltk.download('wordnet')
nltk.download('omw')
nltk.download('averaged_perceptron_tagger')
french_stemmer = SnowballStemmer('french')

# Tokenize, remove stop words, and stem
def process_text(text):
    words = word_tokenize(text)
    filtered = [word for word in words if word not in french_stopwords]
    stemmed = [french_stemmer.stem(word) for word in filtered]
    return ' '.join(stemmed)

# Load data
df = pd.read_json('/content/drive/My Drive/Colab Notebooks/data.json', orient='records')

# Filter out rows with null values
df = df.dropna()

# Process title and description
df['processed_text'] = df['Title'].apply(process_text) + ' ' + df['description'].apply(process_text)

# Vectorize
vectorizer = TfidfVectorizer(stop_words='english', ngram_range=(1, 3), analyzer='word')
X = vectorizer.fit_transform(df['processed_text'])

# K-means clustering
kmeans = KMeans(n_clusters=10).fit(X)
df['cluster'] = kmeans.labels_

# Print the first few rows of the dataframe
print(df.head())

//OUTPUT 
